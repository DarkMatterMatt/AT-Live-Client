{
    "compilerOptions": {
      "module": "ES6",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "target": "ES2017",
      "typeRoots": [
          "node_modules/@types",
          "src/ts/types"
      ],
      "types": [
          "googlemaps",
          "pickr"
      ]
    },
    "include": ["src/ts/*.ts", "gulpfile.js"]
}
